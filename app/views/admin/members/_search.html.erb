<%= search_form_for [:admin, search] do |f| %>
  <div class="member_search_form">
    <div class="form-group row">
      <div class="col-xs-12">
        <%= f.label :last_name_cont, :会員を検索する %><br>
        <%= f.search_field :last_name_cont, placeholder: "例)田中", autofocus: true, class: "form-control" %>
      </div>
    </div>
    <div class="form-group row">
      <div class="col-xs-12">
        <%= f.label :leave_status_eq, :退会ステータス %><br>
        <%= f.select :leave_status_eq, [["有効", 0], ["退会済み", 1]], include_blank: "--選択してください--", autofocus: true, class: "form-control" %>
      </div>
    </div>
    <div class="item_search_btns">
      <%= f.submit "検索", class:"btn btn-info" %>
    </div>
  </div>
<% end %>